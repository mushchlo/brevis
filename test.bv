let print_range := λ(a: int, b)
					if a <= b {
						print(itoa(a));
						if a + 1 <= b {
							print(", ");
							print_range(a + 1, b);
						} else print("\n");
					};

let fizzbuzz := fn (n) {
	if n > 0 {
		fizzbuzz(n - 1)
	};

	if n % 3 == 0 and n % 5 == 0{
		print("Fizzbuzz\n")
	} else if n % 3 == 0 {
		print("Fizz\n")
	} else if n % 5 == 0 {
		print("Buzz\n")
	} else {
		print(itoa(n) ~ "\n")
	}
};

print_range(0,10);
fizzbuzz(100);

let max := λ(z: int, y: int) {
	let ret := 2;
	if {true; 3; z < y} {
		y
	} else {
		z
	};

	ret
};

