let print_range := λ(a: int, b)
					if a <= b {
						print(itoa(a));
						if a + 1 <= b {
							print(", ");
							print_range(a + 1, b);
						} else print("\n");
					};

let fizzbuzz := fn (n, accumulator) {
	accumulator :=
		(if n % 3 == 0 and n % 5 == 0 {
			"Fizzbuzz\n"
		} else if n % 3 == 0 {
			"Fizz\n"
		} else if n % 5 == 0 {
			"Buzz\n"
		} else {
			itoa(n) ~ "\n"
		}) ~ accumulator;
	if n == 0 {
		accumulator
	} else {
		fizzbuzz(n - 1, accumulator);
	}
};

print_range(0,10);
print(fizzbuzz(100, ""));

let max := λ(z: int, y: int) {
	let ret := 2;
	if {true; 3; z < y} {
		y
	} else {
		z
	};

	ret
};

let fx := {3;
